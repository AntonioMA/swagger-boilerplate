{
    "extends": "airbnb-base",
    "rules" : {
        "global-require": 0,
        "function-paren-newline": 0,
        "no-return-assign": 0,
        "prefer-template": 0,
        "dot-location": 0,
        "no-param-reassign": 0,
        "import/no-dynamic-require": 0,
        "prefer-spread": 0,
        "padded-blocks": 0,
        "no-unused-expressions": 0,
        "func-names": 0,
        "no-console": 0,
        "comma-dangle": ["error", {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "ignore"
        }],
        "space-before-function-paren": 0,
        "no-bitwise": 0,
        "no-mixed-operators": 0,
        "no-plusplus": 0,
        "no-await-in-loop": 0
    }
}
